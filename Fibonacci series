n = int(input("How many terms? "))

a = 0
b = 1

count = 0

if n <= 0:
    print("Enter a positive number")
elif n == 1:
    print("Fibonacci sequence up to", n, ":")
    print(a)
else:
    print("Fibonacci sequence:")
    while count < n:
        print(a, end=" ")
        c = a + b
        a = b
        b = c
        count += 1
